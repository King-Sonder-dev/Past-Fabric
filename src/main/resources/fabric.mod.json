  "description": "A utility mod for the latest versions of minecraft, geared towards Anarchy Servers.",
  "authors": [
    "1nject",
    "olliem5"
  ],
  "environment": "client",
  "icon": "assets/pastclient/pastbanner.png",
  "accessWidener": "pastclient.accesswidener",
